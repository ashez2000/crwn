generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:foobar.db"
}

model User {
  id       String @id @default(cuid())
  name     String
  email    String @unique
  password String
}

model Product {
  id       String @id @default(cuid())
  name     String @unique
  price    Int
  image    String
  category String
}
